{{#with person}}
<div class="card ms-5" style="width: 70rem;">
    <div class="card-header bg-primary text-light">
        <h1>
            {{name}}
        </h1>
    </div>
    <div class="card-body mt-3">
        <p>
            <b>
                Age: </b>{{age}}

        </p>
        <p>
            <b>
                Gender: </b>{{gender}}

        </p>

        {{#with address}}
        <address>
            <p><b>Address:</b></p>
            <div>
                {{street}}
            </div>
            <div>
                {{ city }},
            </div>

            <abbr title="New York state">
                {{state}}
            </abbr>

            <div>
                Zip-code: {{zip}}
            </div>

            <abbr title="United States of America">
                {{country}}
            </abbr>

        </address>
        {{/with}}



    </div>
    <div class="card mt-5">
        {{#each education}}

        <div class="card-header bg-warning">
            <h2> Education</h2>

        </div>
        <div class="card-body mt-3">
            <p>
                <b>Degree:</b> {{degree}}
            </p>
            <p>
                <b> {{major}}</b>
            </p>
            <p><strong>University: </strong>{{university}}</p>
            <time><b> Graduation year:</b> 2012</time>
        </div>
        {{/each}}

        {{#each workExperience}}
        <div class="card mt-5">


            <div class="card-header bg-dark text-light">
                <p>
                <h2>Work experience</h2>
                </p>
            </div>
            <div class="card-body mt-4">
                <div>
                    <p>
                        <b>Company:</b> {{company}}
                    </p>
                </div>
                <div>
                    <p>
                        <b>Position:</b> {{title}}
                    </p>
                </div>
                <time>
                    <b>Duration:</b> {{startDate}} - {{endDate}}
                </time>

                <div class="mt-4">
                    <p> <b>Responsibilities:</b></p>
                    {{#each responsibilities}}
                    <ul>
                        <li>
                            {{[]}}
                        </li>
                    </ul>

                    {{/each}}

                </div>
                <p>
                    <b> Year founded:</b> <time>{{year_founded}}</time>
                </p>
                <p>
                    <b>Industry:</b> {{industry}}
                </p>



                <div class="card mt-5">
                    {{#each employees}}
                    <div class="card-header bg-info">


                        <div class="mb-3">
                            <h3>
                                Employees
                            </h3>
                        </div>
                        <div>


                            <p>
                                <strong>
                                    Name:
                                </strong>
                                {{name}}
                            </p>
                            <p>
                                <b>Position:</b> {{position}}
                            </p>
                            <p>
                                <b>Department:</b> {{department}}
                            </p>

                        </div>
                    </div>

                    <div class="card-body mt-3">
                        <div class="card">

                            <div class="card-header">
                                <div>
                                    <h3>
                                        Projects
                                    </h3>
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="row mt-4">
                                    <div class="col-7 ps-5">
                                        {{#each projects}}



                                        <p>
                                            <b>Name:</b>
                                            {{name}}
                                        </p>
                                        <p>
                                            <b>Description:</b> {{description}}
                                        </p>
                                        <div class="mb-3">
                                            <b>Status:</b> {{status}}
                                        </div>

                                        {{#each teams}}
                                        <p><b>Team:</b></p>
                                        <ul>

                                            <div>
                                                <li>
                                                    <p>
                                                        <strong>
                                                            Team name:
                                                        </strong>
                                                        "{{team_name}}"
                                                    </p>
                                            </div>

                                            {{#with team_leader}}
                                            <p>
                                                <b>{{name}}</b> ({{title}})
                                            </p>

                                            <div class="mb-4">
                                                <a href="mailto:{{email}}"> {{email}}</a>
                                            </div>
                                            {{/with}}
                                            </li>


                                            {{#each team_members}}
                                            <p> <b>Members:</b></p>
                                            <ul>

                                                <li>
                                                    <p>
                                                        <b>
                                                            {{name}}
                                                        </b>
                                                        ({{title}})
                                                    </p>

                                                    <p>
                                                        <a href="mailto:{{email}}"> {{email}}</a>
                                                    </p>
                                                    <div>

                                                        <p>
                                                            <b>Skills</b>
                                                        </p>

                                                        {{#each skills}}
                                                        <ul>
                                                            <li>
                                                                {{[]}}
                                                            </li>
                                                        </ul>
                                                        {{/each}}
                                                    </div>
                                                </li>
                                            </ul>
                                        </ul>


                                    </div>

                                    <div class="col-4">
                                        {{#each projects}}
                                        <p>
                                            <b>
                                                Name:
                                            </b>
                                            {{name}}
                                        </p>
                                        <p>
                                            <b>Description:</b> {{description}}
                                        </p>

                                        <div class="mt-3">
                                            <p>
                                                <b> Technologies</b>
                                            </p>
                                        </div>
                                        <div>
                                            {{#each technologies}}
                                            <ul>
                                                <li>
                                                    {{[]}}
                                                </li>
                                            </ul>
                                            {{/each}}
                                        </div>

                                        <div class="mt-3">
                                            <p>
                                                <b>Team members</b>
                                            </p>
                                        </div>
                                        {{#each team_members}}
                                        <ul>
                                            <li>
                                                <div class="mb-3">
                                                    <b>{{name}}</b>
                                                </div>
                                                {{title}}

                                            </li>
                                        </ul>
                                        {{/each}}

                                        {{/each}}
                                    </div>
                                    {{/each}}


                                    {{/each}}
                                    {{/each}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        {{/each}}
    </div>
</div>
</li>

</div>
{{/with}}