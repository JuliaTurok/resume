<div>
    <section>
        <h1>{{name}}</h1>
        <time>{{birthdate}} - {{deathdate}}</time>
        <div class="mt-3">
            <p>
                <b> Birthplace:</b> {{birthplace}}
            </p>
        </div>
        <div>
            <p>
                <b> Deathplace:</b> {{deathplace}}
            </p>
        </div>
        <div>
            <p>
                <b> Nationality:</b> {{nationality}}
            </p>
        </div>
    </section>
    <hr>
    <section>
        <div>
            <p>
                <b> Occupation:</b> {{occupation}}
            </p>
        </div>

        <div>

            <p><b>Known for</b></p>
            {{#each known_for}}
            <ul>
                <li>
                    {{[]}}
                </li>
            </ul>
            {{/each}}
        </div>

        <div class="mt-3 mb-5">
            <p><strong>Education:</strong></p>
            {{#each education}}

            <p><b>Degree:</b> {{degree}}</p>
            <p><b>Field:</b> {{field}}</p>
            <p><b>Institution:</b> {{institution}}</p>
            <time>
                <b>Year:</b> {{year}}
            </time>
            {{/each}}
        </div>

        <div>
            <p><strong>Notable publications:</strong></p>
            {{#each notable_publications}}
            <ul>
                <li>
                    <div class="mb-3">
                        <cite>{{title}}</cite>
                    </div>
                    <p>
                        Year:
                        <time>
                            {{year}}
                        </time>
                    </p>
                    <div>
                        Publisher: {{publisher}}
                    </div>
                </li>
            </ul>
            {{/each}}
        </div>
    </section>
    <hr>
    <section>
        <div>
            <strong>Relationships:</strong>
            {{#each partners}}
            <ul>
                <li>
                    <div class="mb-3 mt-3"><b> {{name}}</b></div>
                    <p>{{relationship}}</p>
                    <time>{{years}}</time>
                </li>
            </ul>
            {{/each}}
        </div>
    </section>
    <hr>
    <section>
        <div>
            <p>
                <strong>
                    Awards:
                </strong>
            </p>
        </div>
        {{#each awards}}
        <div class="mb-3">
            <b>{{title}}</b>
        </div>
        <p> <b>Year:</b> {{year}}</p>
        <b>Description:</b> <q>{{description}}</q>
        {{/each}}

        <div class="mt-5 mb-3">
            <p><strong>Influences:</strong></p>
            {{#each influences}}
            <ul>
                <li>{{[]}}</li>
            </ul>
            {{/each}}
        </div>

        <div class="mb-5 mt-5">
            <p><strong>Influenced:</strong></p>
            {{#each influenced}}
            <ul>
                <li>{{[]}}</li>
            </ul>
            {{/each}}
        </div>

        <article>
            <p><strong>Quotes:</strong></p>
            {{#each quotes}}
            <ul>
                <li>
                    <p><q>{{[]}}</q></p>
                </li>
            </ul>
            {{/each}}
        </article>
        <hr class="mt-5">
        <div class="mt-5">
            <p><strong>Major discovery:</strong></p>
            {{#each major_discoveries}}
            <div>
                <strong>Title:</strong> {{title}}
            </div>
            <div>

                <p><b>Year:</b> {{year}}</p>
            </div>
            <div>
                <p> <b>Description:</b></p>

                {{description}}
            </div>
            {{/each}}
        </div>

        <div class="mt-3">
            {{#with contributions}}
            <p><b>{{title}}</b></p>
            <div>
                <p> <b>Description:</b> </p>
                {{description}}
            </div>
            <div>
                <p> <b>Time:</b> <time>{{year}}</time></p>
            </div>
            <div><b>Field:</b> {{field}}</div>
            {{/with}}
        </div>
    </section>
</div>