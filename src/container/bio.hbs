<div>
    <section>
        <h1>{{name}}</h1>
        <time>{{birthdate}} - {{deathdate}}</time>
        <div>
            <p>
                <b> Birthplace:</b> {{birthplace}}
            </p>
        </div>
        <div>
            <p>
                <b> Deathplace:</b> {{deathplace}}
            </p>
        </div>
        <div>
            <p>
                <b> Nationality:</b> {{nationality}}
            </p>
        </div>
    </section>
    <hr>
    <section>
        <div>
            <p>
                <b> Occupation:</b> {{occupation}}
            </p>
        </div>

        <div>

            <b>Known for</b>
            {{#each known_for}}
            <ul>
                <li>
                    {{[]}}
                </li>
            </ul>
            {{/each}}
        </div>
        <br>
        <div>
            {{#each education}}
            <strong>Education:</strong>
            <p><b>Degree:</b> {{degree}}</p>
            <p><b>Field:</b> {{field}}</p>
            <p><b>Institution:</b> {{institution}}</p>
            <time>
                <b>Year:</b> {{year}}
            </time>
            {{/each}}
        </div>
        <br>
        <br>
        <div>
            <strong>Notable publications:</strong>
            {{#each notable_publications}}
            <ul>
                <li>
                    <div>
                        <cite>{{title}}</cite>
                    </div>
                    <p>
                        Year:
                        <time>
                            {{year}}
                        </time>
                    </p>
                    <div>
                        Publisher: {{publisher}}
                    </div>
                </li>
            </ul>
            {{/each}}
        </div>
    </section>
    <hr>
    <section>
        <div>
            <strong>Relationships:</strong>
            {{#each partners}}
            <ul>
                <li>
                    <div><b> {{name}}</b></div>
                    <p>{{relationship}}</p>
                    <time>{{years}}</time>
                </li>
            </ul>
            {{/each}}
        </div>
    </section>
    <hr>
    <section>
        <div>
            <p>
                <strong>
                    Awards:
                </strong>
            </p>
        </div>
        {{#each awards}}
        <div>
            <b>{{title}}</b>
        </div>
        <p> <b>Year:</b> {{year}}</p>
        <b>Description:</b> <q>{{description}}</q>
        {{/each}}
        <br>
        <br>
        <div>
            <p><strong>Influences:</strong></p>
            {{#each influences}}
            <ul>
                <li>{{[]}}</li>
            </ul>
            {{/each}}
        </div>
        <br>
        <div>
            <p><strong>Influenced:</strong></p>
            {{#each influenced}}
            <ul>
                <li>{{[]}}</li>
            </ul>
            {{/each}}
        </div>
        <br>
        <article>
            <p><strong>Quotes:</strong></p>
            {{#each quotes}}
            <ul>
                <li>
                    <p><q>{{[]}}</q></p>
                </li>
            </ul>
            {{/each}}
        </article>
        <br>
        <br>
        <div>
            <p><strong>Major discovery:</strong></p>
            {{#each major_discoveries}}
            <div>
                <strong>Title:</strong> {{title}}
            </div>
            <div>

                <p><b>Year:</b> {{year}}</p>
            </div>
            <div>
                <p> <b>Description:</b></p>

                {{description}}
            </div>
            {{/each}}
        </div>
        <br>
        <div>
            {{#with contributions}}
            <p><b>{{title}}</b></p>
            <div>
                <p> <b>Description:</b> </p>
                {{description}}
            </div>
            <div>
                <p> <b>Time:</b> <time>{{year}}</time></p>
            </div>
            <div><b>Field:</b> {{field}}</div>
            {{/with}}
        </div>
    </section>
</div>